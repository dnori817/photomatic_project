<form class = "Form" method = "post">
	<% if(typeof error !== undefined) { %>
		<div class= "error"><%= error %></div>
	<%}%>
		Username
		<input class="signup-username" type="text" name = "username" placeholder="Please Enter A Username"></input><br>
		Password
		<input class="signup-password" type="password" name = "password" 					placeholder="Please Enter A Password"></input><br>

	<button>Submit</button>
</form>
