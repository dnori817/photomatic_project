

<form class = "Form" method="post">
	<% if(typeof error !== undefined) { %>
		<div class= "error"><%= error %></div>
	<%}%>
	<div class="username">
		Username
		<input class="login-username" type="text" name = "login-username"
		placeholder="Please Enter Your Username"></input><br>
	</div>

	<div class="password">
		Password
		<input class="login-password" type="text" name = "login-password"
		placeholder="Please Enter Your Password"></input><br>
	</div>

	<button class="submit">Submit</button>

</form>
