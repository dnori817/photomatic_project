
<link rel="stylesheet" types="text.css" href="/css/cssReset.css">
<link rel="stylesheet" types="text.css" href="/css/stylesheet.css">
<link rel="stylesheet" types="text.css" href="/css/normalize.css">
<link rel="stylesheet" types="text.css" href="/css/skeleton.css">

<form class="upload" enctype="multipart/form-data" method="post">
	<% if (typeof error !== "undefined") { %>
		<p class="upload-error">
			<%- error %>
		</p>
	<% } %>
	<input type="file" name="file"/>
	<div class = "button" id="upload">Upload</div>
</form>
